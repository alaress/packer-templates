{
  "builders": [
    {
      "disk_size": 10140,
      "virtualbox_version_file": ".vbox_version",
      "http_directory": "http",
      "boot_command": [
        "linux automatic=met:http,netw:dhcp,hos:veewee,ser:mirror.internode.on.net,dir:pub/mageia/distrib/2/x86_64, kickstart=http://{{ .HTTPIP }}:{{ .HTTPPort }}/auto_inst.cfg<enter><wait>"
      ],
      "ssh_username": "vagrant",
      "guest_os_type": "RedHat_64",
      "ssh_port": 22,
      "ssh_wait_timeout": "10000s",
      "iso_url": "http://mirror.internode.on.net/pub/mageia/distrib/2/x86_64/install/images/boot-nonfree.iso",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "512"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "2"
        ]
      ],
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "iso_md5": "4a0ed5f18c1aa20f42728efd995ba040",
      "type": "virtualbox",
      "shutdown_command": "echo 'shutdown -h now' > shutdown.sh; echo 'vagrant'|sudo -S sh 'shutdown.sh'",
      "boot_wait": "4s",
      "ssh_password": "vagrant"
    }
  ],
  "provisioners": [
    {
      "override": {
        "virtualbox": {
          "execute_command": "echo 'vagrant'|sudo -S sh '{{.Path}}'"
        }
      },
      "scripts": [
        "scripts/postinstall.sh"
      ],
      "type": "shell"
    }
  ]
}